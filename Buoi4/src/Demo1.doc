import java.util.Scanner;

public class Demo1 {
	public int inPut() {
		int num;
		do {
			System.out.print("Nhap vao mang > 0: ");
			Scanner sc = new Scanner(System.in);
			num = sc.nextInt();
		} while (num < 0);

		return num;
	}

	public void nhapMang(int[] a) {
		int so1;
		Scanner in = new Scanner(System.in);
		for (int i = 0; i < a.length; i++) {
			System.out.print("a[" + i + "] = ");
			so1 = in.nextInt();
			a[i] = so1;
		}
	}

	public void xuatMang(int[] a) {
		for (int i = 0; i < a.length; i++)
			System.out.print(a[i] + " ");
		System.out.print("\n");
	}

	public void sapXepTang(int[] a) {
		int temp;
		for (int i = 0; i < a.length; i++) {
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] > a[j]) {
					
					temp = a[i];
					a[i] = a[j];
					a[j] = temp;
				}
			}
		}
	}

	public int timMax(int[] a) {
		int max;
		max = a[0];
		for (int i = 1; i < a.length; i++) {
			if (a[i] > max) {
				max = a[i];
			}
		}
		return max;
	}

	public int timMin(int[] a) {
		int min;
		min = a[0];
		for (int i = 1; i < a.length; i++) {
			if (a[i] < min) {
				min = a[i];
			}
		}
		return min;
	}

	public static void main(String[] args) {
		Demo1 obj = new Demo1();
		int n;
		n = obj.inPut();

		int a[] = new int[n];

		obj.nhapMang(a);

		System.out.println("Mang vua nhap la: ");
		obj.xuatMang(a);
//		System.out.println("=======================");
		
		System.out.println("Sap xep mang tang");
		obj.sapXepTang(a);
		obj.xuatMang(a);

		System.out.println("So lon nhat trong mang: " + obj.timMax(a));
		System.out.println("So be nhat trong mang: " + obj.timMin(a));

	}
}